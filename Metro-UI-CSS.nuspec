    <metadata>
        <id>Metro-UI-CSS</id>
        <version>0.9.5.2</version>
        <title>Metro UI CSS</title>
        <authors>Sergey Pimenov,Sergey Volkov,Valerio Battaglia</authors>
        <owners>sirkirby</owners>
        <licenseUrl>https://raw.github.com/olton/Metro-UI-CSS/master/LICENSE</licenseUrl>
